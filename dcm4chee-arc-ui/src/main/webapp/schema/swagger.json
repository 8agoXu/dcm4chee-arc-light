    "/aets/{aet}/rs/mwlitems": {
      "get": {
        "tags": [
          "MWL-RS"
        ],
        "summary": "Search for Scheduled Procedure Steps",
        "operationId": "SearchForSPS",
        "produces": [
          "multipart/related;type=application/dicom+xml",
          "multipart/related;type=application/json"
        ],
        "parameters": [
          {
            "name": "aet",
            "in": "path",
            "description": "Archive AE Title",
            "required": true,
            "type": "string"
          },
          {
            "name": "{attributeID}",
            "in": "query",
            "description": "{attributeID}={value}; {attributeID} = {dicomTag} | {dicomKeyword} | {dicomTag}.{attributeID} | {dicomKeyword}.{attributeID}",
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          },
          {
            "name": "includefield",
            "in": "query",
            "description": "includefield={attributeID} | all",
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          },
          {
            "name": "orderby",
            "in": "query",
            "description": "orderby=[-]{attributeID}",
            "type": "array",
            "items": {
              "type": "string"
            },
            "collectionFormat": "multi"
          },
          {
            "name": "fuzzymatching",
            "in": "query",
            "description": "fuzzy semantic matching of person names",
            "type": "boolean",
            "default": false
          },
          {
            "name": "offset",
            "in": "query",
            "description": "offset={skippedResults}",
            "type": "integer",
            "default": 0
          },
          {
            "name": "limit",
            "in": "query",
            "description": "limit={maximumResults}",
            "type": "integer"
          }
        ],
        "responses": {
          "200": {
            "description": "Matching Scheduled Procedure Steps",
            "schema": {
              "type": "file"
            }
          },
          "default": {
            "description": "unexpected error"
          }
        }
      },
      "post": {
        "tags": [
          "MWL-RS"
        ],
        "summary": "Create/Update Scheduled Procedure Step",
        "operationId": "CreateUpdateSPS",
        "consumes": [
          "application/json"
        ],
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "aet",
            "in": "path",
            "description": "Archive AE Title",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "Created/Updated Scheduled Procedure Step",
            "schema": {
              "type": "file"
            }
          },
          "default": {
            "description": "unexpected error"
          }
        }
      }
    },
