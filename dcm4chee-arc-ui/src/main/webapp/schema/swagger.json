    "/queue/{queueName}": {
      "get": {
        "tags": [
          "QUEUE-RS"
        ],
        "summary": "list messages in queue",
        "operationId": "listMessages",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "queueName",
            "in": "path",
            "description": "name of queue",
            "required": true,
            "type": "string"
          },
          {
            "name": "status",
            "in": "query",
            "description": "status of messages to filter by",
            "type": "string",
            "enum": [
              "SCHEDULED",
              "IN PROCESS",
              "COMPLETED",
              "WARNING",
              "FAILED",
              "CANCELED"
            ]
          },
          {
            "name": "updatedBefore",
            "in": "query",
            "description": "maximum update date of messages to filter by. Format: YYYY-MM-DD",
            "type": "string",
            "format": "date"
          },
          {
            "name": "offset",
            "in": "query",
            "description": "number of skipped messages before first message in returned list",
            "type": "integer"
          },
          {
            "name": "limit",
            "in": "query",
            "description": "maximal number of messages in returned list",
            "type": "integer"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "messageList.schema.json"
            }
          },
          "404": {
            "description": "Queue not found"
          },
          "default": {
            "description": "unexpected error"
          }
        }
      },
      "delete": {
        "tags": [
          "QUEUE-RS"
        ],
        "summary": "delete messages in queue",
        "operationId": "deleteMessages",
        "produces": [
          "application/json"
        ],
        "parameters": [
          {
            "name": "queueName",
            "in": "path",
            "description": "name of queue",
            "required": true,
            "type": "string"
          },
          {
            "name": "status",
            "in": "query",
            "description": "status of messages to filter by",
            "type": "string",
            "enum": [
              "SCHEDULED",
              "IN PROCESS",
              "COMPLETED",
              "WARNING",
              "FAILED",
              "CANCELED"
            ]
          },
          {
            "name": "updatedBefore",
            "in": "query",
            "description": "maximum update date of messages to filter by. Format: YYYY-MM-DD",
            "type": "string",
            "format": "date"
          }
        ],
        "responses": {
          "200": {
            "description": "successful operation",
            "schema": {
              "$ref": "#/definitions/Deleted"
            }
          },
          "404": {
            "description": "Queue not found"
          },
          "default": {
            "description": "unexpected error"
          }
        }
      }
    },
